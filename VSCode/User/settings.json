{
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.languageServer": "Pylance",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "workbench.iconTheme": "vscode-icons",
    "editor.fontSize": 16,
    "tabnine.experimentalAutoImports": true,
    "glassit.alpha": 255,
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.donotVerifyTags": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "git.autofetch": true,
    "extensions.ignoreRecommendations": true,
    "python.pythonPath": "C:\\Users\\qrwea\\anaconda3\\python.exe",
    "terminal.integrated.fontWeight": null,
    "terminal.integrated.automationShell.linux": "",
    "terminal.integrated.fontSize": 12,
    "editor.fontLigatures": null,
    "python.defaultInterpreterPath": "C:\\Users\\qrwea\\anaconda3\\python.exe",
    "terminal.integrated.fontFamily": "'MesloLGM NF'",
    "terminal.integrated.shellArgs.windows": "-nologo",
    "workbench.colorTheme": "Visual Studio Dark",
    "code-runner.runInTerminal": true,
    "code-runner.executorMap": {

        //"cpp": "cd $dir; cls; g++ -std=c++17 -Wall -Wextra -O2 -Wfloat-equal -Wshadow -Wduplicated-cond -Wcast-qual -Wconversion -Wlogical-op $fileName -o CPP_VSCODE_GEN; .\\CPP_VSCODE_GEN"
        "cpp": "cd $dir; cls; g++ -std=c++17 -Wall -Wextra -O2 -Wfloat-equal -Wshadow -Wduplicated-cond -Wcast-qual -Wconversion -Wlogical-op $fileName -o run; Start-Process cmd \"/k run\""

        
        ,"java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
        "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "php": "php",
        "python": "python -u",
        "go": "go run", 
        "powershell": "powershell -ExecutionPolicy ByPass -File",
        "bat": "cmd /c",
        "shellscript": "bash",
        "typescript": "ts-node",
        "scala": "scala",
        "swift": "swift",
        "r": "Rscript",
        "applescript": "osascript",
        "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
        "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        "haskell": "runhaskell",
        "sass": "sass --style expanded",
        "scss": "scss --style expanded",
    },
    "code-runner.saveAllFilesBeforeRun": true,
    "code-runner.preserveFocus": false,
    "bracketPairColorizer.showHorizontalScopeLine": false,
    "bracketPairColorizer.showVerticalScopeLine": false,
    "C_Cpp.addNodeAddonIncludePaths": true,
    "C_Cpp.autocompleteAddParentheses": true,
    "kite.codefinder.enableLineDecoration": true,
    "editor.defaultFormatter": "ms-vscode.cpptools",
    "editor.formatOnPaste": true,
    "diffEditor.wordWrap": "off",
    "C_Cpp.formatting": "vcFormat",
    "C_Cpp.vcFormat.space.insertAfterSemicolon": true,
    "editor.formatOnType": true,
    "kite.showWelcomeNotificationOnStartup": false,
    "C_Cpp.errorSquiggles": "Disabled",
    "C_Cpp.intelliSenseEngineFallback": "Enabled",
    "editor.formatOnSave": true,
    "editor.inlineSuggest.enabled": true,
    "security.workspace.trust.untrustedFiles": "open",
    "bracket-pair-colorizer-2.depreciation-notice": false,
}